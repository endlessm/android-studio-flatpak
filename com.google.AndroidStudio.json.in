						"for i in lib32-debs/*.deb ; do echo $i ; dpkg-deb -x $i lib32root ; done",
						"echo '== Copying 32-bit libraries =='",
						"mv lib32root/lib32/* lib32root/usr/lib32/* /app/lib"
					]
